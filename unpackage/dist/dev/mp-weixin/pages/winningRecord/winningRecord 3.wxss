page {
  width: 100%;
  height: 100%;
}
.winningRecord {
  height: 100%;
  background: url("https://xcxsite.vjifen.com/v/crownCookiesImg/bg2.png") no-repeat center;
  background-size: cover;
}
.wr-center {
  border-radius: 20rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin: 52rpx 30rpx 30rpx 30rpx;
  background: #fff;
  padding: 32rpx;
}
.wrc-listBox {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.wrc-listBox-list {
  padding: 20rpx 0;
  border-bottom: 2rpx solid #dedede;
}
.wrc-listBox-list-mes {
  margin-bottom: 16rpx;
  font-size: 32rpx;
  color: #333;
}
.wrc-listBox-list-time {
  font-size: 24rpx;
  color: #999999;
}
.wrc-listBox-list-price {
  font-size: 24rpx;
  color: #f10000;
}
.wrc-listBox-list-price text {
  font-size: 36rpx;
}
.wrc-listBox-list-danmai {
  font-size: 30rpx;
  color: #f10000;
}
.wrc-titleImg {
  width: 70%;
  margin-bottom: 20rpx;
}
.wrc-nodata {
  margin-top: 54%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.wrc-nodata image {
  width: 100%;
}
.wrc-nodata view {
  font-size: 30rpx;
  color: #656565;
  margin-top: 40rpx;
}
.clickMore {
  text-align: center;
  margin-top: 20rpx;
}
.colorGrey {
  color: #999999;
}

